{
  "settingsInheritedFrom": "svg-GHC-2/whitesource-config@master",
  "scanSettings": {
    "configMode": "AUTO",
    "configExternalURL": "",
    "baseBranches": [
      "main",
      "develop"
    ],
    "enableLicenseViolations": true,
    "enableReachability": true
  },
  "checkRunSettings": {
    "vulnerableCheckRunConclusionLevel": "failure",
    "displayMode": "diff"
  },
  "issueSettings": {
    "minSeverityLevel": "LOW",
    "issueType": "DEPENDENCY"
  },
  "remediateSettings": {
    "baseBranches": [
      "main"
    ],
    "workflowRules": {
      "enabled": true
    },
    "enableRenovate": true,
    "timezone": "America/New_York",
    "extends": [
      ":dependencyDashboard"
    ],
    "transitiveRemediation": true,
    "packageRules": [
      {
        "matchDataSources": [
          "Docker"
        ],
        "schedule": [
          "after 10pm",
          "before 5:00am"
        ]
      },
      {
        "matchUpdateTypes": [
          "digest"
        ],
        "matchConfidence": [
          "very high",
          "high",
          "Neutral"
        ],
        "automerge": false
      },
      {
        "matchUpdateTypes": [
          "patch",
          "pin",
          "minor"
        ],
        "automerge": false
      },
      {
        "matchDepTypes": [
          "devDependencies"
        ],
        "automerge": false
      },
      {
        "matchConfidence": [
          "low"
        ],
        "dependencyDashboardApproval": true
      }
    ]
  }
}