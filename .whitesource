{
  "settingsInheritedFrom": "svg-GHC-2/whitesource-config@master",
  "scanSettings": {
    "configMode": "AUTO",
    "configExternalURL": "",
    "baseBranches": [
      "main",
      "develop"
    ],
    "enableLicenseViolations": true
  },
  "checkRunSettings": {
    "vulnerableCheckRunConclusionLevel": "failure",
    "displayMode": "diff"
  },
  "issueSettings": {
    "minSeverityLevel": "LOW",
    "issueType": "DEPENDENCY"
  },
  "remediateSettings": {
    "baseBranches": [
      "main"
    ],
    "workflowRules": {
      "enabled": true
    },
    "enableRenovate": true,
    "extends": [
      ":dependencyDashboard",
      ":approveMajorUpdates"
      ":semanticPrefixFixDepsChoreOthers",
      ":autodetectPinVersions",
      ":prConcurrentLimit20",
      ":prHourlyLimitNone",
      ":automergeTypes",
      "group:monorepos",
      "group:recommended",
      "workarounds:all",
      "github>whitesource/merge-confidence:beta"
    ],
    "ignorePaths": [
      "**/node_modules/**",
      "**/bower_components/**",
      "**/vendor/**",
      "**/examples/**",
      "**/__fixtures__/**"
    ],
    "transitiveRemediation": true,
    "packageRules": [
      {
        "matchUpdateTypes": [
          "digest"
        ],
        "matchConfidence": [
          "very high",
          "high",
          "Neutral"
        ],
        "automerge": true
      },
      {
        "matchUpdateTypes": [
          "patch",
          "pin",
          "minor"
        ],
        "automerge": true
      },
      {
        "matchDepTypes": [
          "devDependencies"
        ],
        "automerge": true
      },
      {
        "matchManagers": [
          "npm"
        ],
        "rangeStrategy": "pin"
      },
      {
        "matchConfidence": [
          "low"
        ],
        "dependencyDashboardApproval": true
      }
    ]
  }
}