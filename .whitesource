{
  "scanSettings": {
    "configMode": "AUTO",
    "configExternalURL": "",
    "baseBranches": ["main"],
    "enableLicenseViolations": true
  },
  "checkRunSettings": {
    "vulnerableCheckRunConclusionLevel": "failure",
    "displayMode": "diff"
  },
  "issueSettings": {
    "minSeverityLevel": "LOW",
    "issueType": "DEPENDENCY"
  },
  "remediateSettings": {
    "workflowRules": {
      "enabled": true
    },
    "enableRenovate": true,
    "extends": ["config:base"],
    "packageRules": [
      {
        "enabled": false,
        "matchUpdateTypes": [
          "major"
        ]
      }
    ]
  }
}
